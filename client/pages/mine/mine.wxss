/* pages/community/mine.wxss */

page{
  background: #ffdd40;
}

.medals {
  /* Rectangle: */
  background: #ffdd40;
  border: 1px solid #ffdd40;
  display:flex;
  flex-wrap:wrap;
  justify-content:space-around;
  padding-top: 30px;
}
.medal-text{
  font-family:PingFangSC-Regular;
font-size:12px;
color:#666666;
letter-spacing:1.11px;
text-align:center;
}
.medal-not{
  -webkit-filter: grayscale(1); /* Webkit */
  filter: gray; /* IE6-9 */  
  filter: grayscale(1); /* W3C */
}
.medal {
  width: 40%;
  overflow: hidden;
  display: inline-block;
  text-align: center;
}
.medal-img{
  width: 100%;
}

.some-btn {
  float: right;
}

.btn {
  background: #ffdd40;
  border-radius: 14px;
  width: 85px;
  height: 31px;
  overflow: hidden;
  font-family: PingFangSC-Regular;
  font-size: 16px;
  color: #666;
  letter-spacing: 0.44px;
  text-align: center;
  line-height: 31px;
  width: 100px;
  margin: 20px;
}

.description {
  font-family: PingFangHK-Regular;
  font-size: 17px;
  color: #969696;
  letter-spacing: 0.57px;
  line-height: 20px;
  text-align: left;
  margin: 10px 20px;
}

.audio {
  background: #fff;
  border-radius: 13px;
  height: 30px;
  margin-top: 0px;
  margin-right: 15px;
  display: inline-flex;
  float: right;
  width: 75%;
  justify-content: flex-end;
  align-items: center;
}

.audio-icon {
  /*float:right;*/
  height: 25px;
  width: 25px;
  align-items: center;
  padding-right: 10px;
}

.tags {
  text-align: right;
  margin-right: 15px;
}

.tag {
  background: #d7d7d7;
  border-radius: 10px;
  height: 25px;
  font-family: PingFangHK-Regular;
  font-size: 15px;
  color: #fff;
  letter-spacing: 0;
  text-align: left;
  margin: 4px;
  display: inline;
  padding: 1px 6px;
}

.time {
  display: inline;
  float: left;
  font-family: PingFangHK-Regular;
  font-size: 13px;
  color: #999;
  letter-spacing: 0;
  text-align: left;
  height: 30px;
  margin-top: 5px;
  margin-left: 16px;
  width: 15%;
}

.holder {
  overflow: hidden;
}


.btn-group {
  display: inline-flex;
  align-items: center;
  margin-right: 8px;
}

.icon-sm {
  width: 20px;
  height: 20px;
}

.avatar-big {
  width: 57px;
  height: 57px;
  border-radius: 50%;
}

.btn-num {
  height: 20px;
  line-height: 20px;
}

.info-card {
  background: white;
  position: relative;
  overflow: hidden;
  padding-left: 20px;
  margin-left: 0;
  margin-right: 0;
}

.info-detail-group {
  float: left;
}

.avatar {
  float: left;
  margin: 5px;
}

.share-btn {
  float: right;
  padding-top: 25px;
  padding-right: 15px;
}

.info-group {
  overflow: hidden;
  display: inline-flex;
  align-items: center;
  width: 50%;
  padding-left: 0px;
  padding-top: 20px;
}

.info-name {
  font-family: PingFangHK-Medium;
  font-size: 20px;
  color: #999;
  letter-spacing: 0.42px;
  text-align: left;
}

.info-date {
  font-family: PingFangHK-Regular;
  font-size: 11px;
  color: #b4b4b4;
  letter-spacing: 0;
  text-align: left;
}

.location {
  font-family: PingFangHK-Regular;
  font-size: 12px;
  color: #b4b4b4;
  letter-spacing: 0;
  text-align: left;
  padding-left: 20px;
  float: left;
  height: 35px;
}

.info-card-out {
  position: relative;
}

.background {
  position: absolute;
  left: 0;
  /*right: 0;*/
  top: 0;
  /*bottom: 0;*/
  opacity: 0.1;
  height: 100%;
  width: 100%;
  border-radius: 14px;
}

